<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from PasswordValidatorService WSDL service -->

  <service name="PasswordValidator" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service"/>
      <parameter name="wsdlServiceElement" value="PasswordValidatorService"/>
      <parameter name="schemaQualified" value="http://service"/>
      <parameter name="wsdlServicePort" value="PasswordValidator"/>
      <parameter name="className" value="service.PasswordValidatorSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="PasswordValidator"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="validate" qname="operNS:validate" xmlns:operNS="http://service" returnQName="retNS:validateReturn" xmlns:retNS="http://service" returnType="rtns:boolean" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:password" xmlns:pns="http://service" type="tns:Password" xmlns:tns="http://service"/>
      </operation>
      <parameter name="allowedMethods" value="validate"/>

      <typeMapping
        xmlns:ns="http://service"
        qname="ns:Password"
        type="java:service.Password"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
